/* parameters for the nmj simulation */

INTERACTION_RADIUS = 0.003

iterations = 6300000                           /*number of iterations */
                                                                                            
default_onrate=0.22E8   /*default onrate for vesicles ca_binding_sites*/
default_offrate=910.0  /*default offrate for vesicles ca_binding_sites*/
                                                                                             
divider = 1                                    /* factor used to modify vesicle ca_binding_rates */
sensor_onrate=default_onrate*divider     /*final onrate for ca_binding sites*/
sensor_offrate=default_offrate*divider   /*final offrate for ca binding sites*/
ca_diff_const=6E-6        /*calcium diffusion constant*/

sensor_Y_onrate = 10E6
sensor_Y_offrate = 15

fixed_buffer_onrate = 1E8                      /* onrate for fixed ca buffer sites */ 
fixed_buffer_offrate = 10E3                    /* offrate for fixed ca buffer sites */
fixed_buffer_concentration = 0.0020             /* concentration of fixed buffer in Molar */ 
fixed_buffer_diff_const=0

mobile_buffer_onrate = 1E8		       /* onrate for mobile ca buffer molecules */
mobile_buffer_offrate = 10E3                   /* offrate for mobile ca buffer molecules */
mobile_buffer_concentration = 0.00          /* concentration of mobile buffer in Molar */
mobile_buffer_diff_const = 0	               /* mobile buffer diffusion constant */                                                                                             
/*mobile_buffer_diff_const = 0*/

time_step = 1e-8                               /* simulation time step */
maximum_time_step = time_step*1                /* maximum simulation time step - set equal to time step for max accuracy */
reaction_output_time_step = 5e-7               /* interval between reaction data output data points */
effector_grid_density = 10000                  /* density of the effector site grid */

